{
  "dashboard": {
    "title": "k6 Load Testing - ETP Express",
    "tags": ["k6", "load-testing"],
    "timezone": "browser",
    "panels": [
      {
        "title": "Virtual Users",
        "targets": [
          {
            "query": "SELECT last(\"value\") FROM \"vus\" WHERE $timeFilter GROUP BY time($__interval) fill(null)",
            "rawQuery": true,
            "refId": "A",
            "datasource": "InfluxDB-k6"
          }
        ],
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "title": "Request Rate (req/s)",
        "targets": [
          {
            "query": "SELECT mean(\"value\") FROM \"http_reqs\" WHERE $timeFilter GROUP BY time($__interval) fill(null)",
            "rawQuery": true,
            "refId": "A",
            "datasource": "InfluxDB-k6"
          }
        ],
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "title": "Response Time (P95)",
        "targets": [
          {
            "query": "SELECT percentile(\"value\", 95) FROM \"http_req_duration\" WHERE $timeFilter GROUP BY time($__interval) fill(null)",
            "rawQuery": true,
            "refId": "A",
            "datasource": "InfluxDB-k6"
          }
        ],
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "title": "Error Rate",
        "targets": [
          {
            "query": "SELECT mean(\"value\") FROM \"errors\" WHERE $timeFilter GROUP BY time($__interval) fill(null)",
            "rawQuery": true,
            "refId": "A",
            "datasource": "InfluxDB-k6"
          }
        ],
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      }
    ],
    "schemaVersion": 16,
    "version": 0
  }
}
