{
  "name": "etp-express-backend",
  "version": "1.0.0",
  "description": "Backend para ETP Express - Sistema assistivo de elaboração de Estudos Técnicos Preliminares",
  "author": "ETP Express Team",
  "license": "MIT",
  "scripts": {
    "build": "npx -y @nestjs/cli build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/typeorm.config.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/config/typeorm.config.ts",
    "migration:run:prod": "typeorm migration:run -d dist/config/typeorm.config.js",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/typeorm.config.ts",
    "seed:legislation": "ts-node -r tsconfig-paths/register scripts/seed-legislation.ts",
    "seed:admin": "ts-node -r tsconfig-paths/register scripts/seed-admin.ts",
    "seed:admin:prod": "node dist/scripts/seed-admin.js",
    "seed:e2e": "ts-node -r tsconfig-paths/register scripts/seed-e2e-users.ts",
    "seed:e2e:prod": "node dist/scripts/seed-e2e-users.js",
    "seed:alpha-testers": "ts-node -r tsconfig-paths/register scripts/seed-alpha-testers.ts",
    "reset:demo": "ts-node -r tsconfig-paths/register scripts/reset-demo-data.ts",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "swagger:export": "ts-node -r tsconfig-paths/register src/scripts/export-swagger.ts",
    "test:mutation": "stryker run",
    "test:mutation:dry": "stryker run --dryRunOnly"
  },
  "dependencies": {
    "@json2csv/plainjs": "^7.0.6",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.3",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@sentry/nestjs": "^10.25.0",
    "@sentry/node": "^10.25.0",
    "@sentry/profiling-node": "^10.25.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.65.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "exa-js": "^2.0.11",
    "exceljs": "^4.4.0",
    "express-rate-limit": "^8.2.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "^18.0.2",
    "mammoth": "^1.11.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.10",
    "openai": "^6.15.0",
    "opossum": "^9.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-parse": "^2.4.5",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.30.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/jest-runner": "^9.4.0",
    "@stryker-mutator/typescript-checker": "^9.4.0",
    "@types/adm-zip": "^0.5.7",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/jest": "^30.0.0",
    "@types/node-cache": "^4.1.3",
    "@types/nodemailer": "^7.0.4",
    "@types/opossum": "^8.1.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "adm-zip": "^0.5.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^17.0.0",
    "typescript-eslint": "^8.47.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
